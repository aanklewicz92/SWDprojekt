	InconsistentMatrixListener listener;
	
	public SomeClass(InconsistentMatrixListener listener) {
		this.listener = listener;
	}

	public void setPreferences(ArrayList<Double[][]> matrices) {
		//System.out.println("Macierz kryteriów");
